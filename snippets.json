{
	"to": {
		"prefix": ".to",
		"body": [
			".to(${1:selector}, {duration: ${2:1}, ${3:toVars}})"
		],
		"description": "GSAP to Tween"
	},
	"from": {
		"prefix": ".from",
		"body": [
			".from(${1:selector}, {duration: ${2:1}, ${3:fromVars}})"
		],
		"description": "GSAP from Tween"
	},
	"set": {
		"prefix": ".set",
		"body": [
			".set(${1:selector}, {${2:toVars}})"
		],
		"description": "GSAP set Tween"
	},
	"fromTo": {
		"prefix": ".fromTo",
		"body": [
			".fromTo(${1:selector}, {${2:fromVars}}, {duration: ${3:1}, ${4:toVars}})"
		],
		"description": "GSAP fromTo Tween"
	},
	"import gsap": {
		"prefix": "impg",
		"body": [
			"import { gsap } from \"gsap\";${1}"
		],
		"description": "Import GSAP"
	},
	"timeline": {
		"prefix": "tl",
		"body": [
			"const ${1:tl} = gsap.timeline({${2:tlVars}})"
		],
		"description": "GSAP timeline"
	}
}