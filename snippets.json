{
	"to": {
		"prefix": ".to",
		"body": [
			".to(${1:selector},{duration: ${2:1}, ${3:vars}})"
		],
		"description": "GSAP to Tween"
	},
	"from": {
		"prefix": ".from",
		"body": [
			".from(${1:selector},{duration: ${2:1}, ${3:vars}})"
		],
		"description": "GSAP from Tween"
	},
	"set": {
		"prefix": ".set",
		"body": [
			".set(${1:selector},{${2:vars}})"
		],
		"description": "GSAP set Tween"
	},
	"fromTo": {
		"prefix": ".fromTo",
		"body": [
			".fromTo(${1:selector},{${2:vars}},{duration: ${3:1}, ${4:vars}})"
		],
		"description": "GSAP fromTo Tween"
	},
	"import gsap": {
		"prefix": "impg",
		"body": [
			"import { gsap } from \"gsap\";${1}"
		],
		"description": "Import GSAP"
	}
}